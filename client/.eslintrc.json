{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {},

    "overrides": [
        {
            "files": [
                "*.jsx",
                "*.js"
            ]
        },
        {
            "extends": [
                "plugin:cypress/recommended"
            ],
            "files": [
                "cypress/**/*.js"
            ]
        }
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}